<Window x:Name="Dashboard" x:Class="La_Bakéry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:La_Bakéry"
        mc:Ignorable="d"
        Title="Home" Height="768" Width="1366" Background="#FFF2F6F5" Foreground="{DynamicResource {x:Static SystemColors.DesktopBrushKey}}" Padding="10,0" WindowStyle="ThreeDBorderWindow" WindowStartupLocation="CenterScreen">
    <StackPanel x:Name="navPanel" HorizontalAlignment="Left" Margin="0,0,0,-29.6" Width="250" Background="#FFC8DAD3">
        <StackPanel.Effect>
            <DropShadowEffect Opacity="0.5" RenderingBias="Quality"/>
        </StackPanel.Effect>
        <Border CornerRadius="50"/>
        <Ellipse Fill="White" Height="100" Width="100">
            <Ellipse.Stroke>
                <ImageBrush/>
            </Ellipse.Stroke>
        </Ellipse>
    </StackPanel>
</Window>
